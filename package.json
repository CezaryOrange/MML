{
  "name": "Mapping-My-Life",
  "version": "1.0.3",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --bail",
    "watch": "NODE_ENV=development webpack --config webpack.config.js --watch --display-modules --display-chunks",
    "watch:standalone": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8084  --profile  --inline --hot",
    "lint": "standard 'src/**/*.js' 'src/**/*.jsx'",
    "clean": "rm -rf build",
    "copy:manifest": "install -m0644 package.json README.md build",
    "prebuild": "#deactivated npm-run-all lint clean",
    "postbuild": "npm run copy:manifest",
    "prewatch": "npm run clean"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cozy-client-js": "0.4.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "redux-logger": "^3.0.0",
    "standard": "^10.0.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.8.2",
